{
  "name": "student",
  "version": "2025.4.1",
  "description": "Beispiel",
  "author": "Juergen Zimmermann",
  "private": true,
  "license": "GPL-3.0",
  "exports": "./dist/src/main.js",
  "type": "module",
  "engines": {
    "node": ">=23.9.0",
    "npm": ">=11.1.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "start": "node dist/src/main.js",
    "pre:dev": "npm run copy-resources",
    "dev": "nest start --watch",
    "debug": "npm run dev -- --debug",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "set LOG_LEVEL=info&& vitest",
    "asciidoctor": "node scripts/asciidoctor.mts",
    "audit": "npm audit --omit dev",
    "clean": "rimraf dist log/server.log",
    "copy-resources": "node scripts/copy-resources.mts",
    "depcheck": "depcheck",
    "eslint": "eslint src test",
    "eslint:inspect-config": "eslint --inspect-config",
    "eslint:debug": "npm run eslint -- --debug",
    "invoke-webrequest": "pwsh -Command Invoke-WebRequest https://localhost:3000/rest/1 -SslProtocol Tls13 -SkipCertificateCheck",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "madge": "madge --circular src/main.ts",
    "madge:svg": "madge --image dependencies.svg src/main.ts",
    "prettier": "prettier --cache --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:check": "prettier --cache --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "revealjs": "asciidoctor-revealjs .extras/doc/folien/folien.adoc",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "sonar": "node scripts/sonar-scanner.mts",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty"
  },
  "keywords": [
    "Node",
    "npm",
    "Nest",
    "REST",
    "GraphQL",
    "Apollo",
    "Axios",
    "TypeScript",
    "ESLint",
    "PostgreSQL",
    "TypeORM",
    "JWT",
    "Keycloak",
    "class-validator",
    "CSP",
    "Clickjacking",
    "CORS",
    "pino",
    "nodemailer",
    "Jest",
    "ts-jest"
  ],
